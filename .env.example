# Environment configuration example for the agent

# Node.js server configuration
PORT=3000  # Port for the Express server

# GitHub configuration
GITHUB_TOKEN=PR-CYBR-ACTIONS
REPO_OWNER=PR-CYBR
REPO_NAME=PR-CYBR-MGMT-AGENT

# Database configuration (if applicable)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password

# API keys and secrets
API_KEY=your_api_key_here
SECRET_KEY=your_secret_key_here

# Logging configuration
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL

# CORS configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://yourdomain.com

# External service URLs
EXTERNAL_SERVICE_URL=https://api.external-service.com

# Flask configuration (if applicable)
FLASK_ENV=development  # Options: development, production
FLASK_DEBUG=1          # Options: 0 (off), 1 (on)

# Assistant configuration
ASSISTANT_ID=your_default_assistant_id_here

# Discord
DISCORD_WEBHOOK_URL=DISCORD_WEBHOOK_URL

# PR-CYBR configurations
DIV-CODE=DIV_CODE

# Docker settings
DOCKER_USERNAME=your_docker_username
DOCKER_PASSWORD=your_docker_password

# Domain Info
DOMAIN_NAME=your_domain_name_here
SSL_EMAIL=your_ssl_email_here

# N8N Config
N8N_HOST=localhost  # Default host for n8n
N8N_PORT=5678       # Default port for n8n
N8N_BASIC_AUTH_USER=your_n8n_basic_auth_user
N8N_BASIC_AUTH_PASSWORD=your_n8n_basic_auth_password

# NGINX Config
NGINX_CONF=path_to_your_nginx_conf

# Zerotier Config
ZEROTIER_IP=your_zerotier_ip
ZEROTIER_ID=your_zerotier_network_id

# Timezone configuration
N8N_TZ=your_timezone_here  # e.g., Europe/Berlin

# Other configurations
# Add any other environment variables your application might need